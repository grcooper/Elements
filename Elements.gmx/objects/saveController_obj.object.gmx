        one = totalScore;
    }
    else if (totalScore &gt; two)
    {
        five = four;
        four = three;
        three = two;
        two = totalScore;
    }
    else if (totalScore &gt; three)
    {
        five = four;
        four = three;
        three = totalScore;
    }
    else if (totalScore &gt; four)
    {
        five = four;
        four = totalScore;
    }
    else if (totalScore &gt; five)
    {
        five = totalScore;
    }
    
    ini_write_real('highscores', '1', one);
    ini_write_real('highscores', '2', two);
    ini_write_real('highscores', '3', three);
    ini_write_real('highscores', '4', four);
    ini_write_real('highscores', '5', five);
}
