            <string>//Draw Title Shadow
draw_set_font(MoolTitle_fnt);
draw_set_halign(fa_center);
draw_set_color(make_color_rgb(6,38,53));
draw_text((room_width/2) - 5, 33, "Level " + string(curLevel));
draw_text((room_width/2) - 5, 103, "Score");

//Draw Back to Menu Shadow
draw_set_font(MoolReg_fnt);
draw_text((room_width/2) - 5, 693, "Back to Menu");

if (winCond)
{
    //Draw Continue Shadow
    draw_text((room_width/2) - 5, 583, "Continue");
}
    
//Draw Title
draw_set_font(MoolTitle_fnt);
draw_set_color(c_white);
draw_text(room_width/2, 30, "Level " + string(curLevel));
draw_text((room_width/2), 100, "Score");

//Draw Back to Menu
draw_set_font(MoolReg_fnt);
draw_text((room_width/2), 690, "Back to Menu");

if(winCond)
{
    //Draw Continue
    draw_text(room_width/2, 580, "Continue");
}

draw_set_font(scoreCount_fnt);

//Monsters Killed Score
if(mk &lt; eKilledScore)
{  
    mk += 1;
}
//Shadow
draw_set_color(make_color_rgb(6,38,53));
draw_text((room_width/2) - 3, 252, "Monsters Killed: " + string(mk));
//Regular
draw_set_color(c_white);
draw_text(room_width/2, 250, "Monsters Killed: " + string(mk));

//accuracy score
if(as &lt; aScore)
{ 
    as += 1;
}

//Shadow
draw_set_color(make_color_rgb(6,38,53));
draw_text((room_width/2) - 3, 292, "Accuracy: " + string(as));
//Regular
draw_set_color(c_white);
draw_text(room_width/2, 290, "Accuracy: " + string(as));

//Wall Health Score
if(ws &lt; wallScore)
{ 
    ws += 1;
}
//Shadow
draw_set_color(make_color_rgb(6,38,53));
draw_text((room_width/2) - 3, 332, "Wall Health: " + string(ws));
//Regular
draw_set_color(c_white);
draw_text(room_width/2, 330, "Wall Health: " + string(ws));

//Wizard Health Score
if(hs &lt; healthScore)
{ 
    hs += 1;
}
//Shadow
draw_set_color(make_color_rgb(6,38,53));
draw_text((room_width/2) - 3, 372, "Wizard Health: " + string(hs));
//Regular
draw_set_color(c_white);
draw_text(room_width/2, 370, "Wizard Health: " + string(hs));

//Total Score
ts = mk + as + ws + hs + ps;
//Shadow
draw_set_color(make_color_rgb(6,38,53));
draw_text((room_width/2) - 3, 412, "Total Score: " + string(ts));
//Regular
draw_set_color(c_white);
draw_text(room_width/2, 410, "Total Score: " + string(ts));


